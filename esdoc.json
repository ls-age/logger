{
  "source": "src",
  "destination": "docs/api",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "@lsage/logger"
        },
        "accessor": {
          "access": ["public", "package", "private"]
        },
        "test": {
          "type": "mocha",
          "source": "./test/src"
        },
        "manual": {
          "files": [
            "./docs/manual/usage/API.md",
            "./CHANGELOG.md"
          ]
        }
      }
    },
    {
      "name": "esdoc-plugin-require-coverage"
    }
  ]
}
